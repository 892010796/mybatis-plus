<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="beanPostProcessor" class="com.example.mybatisplustest.beans.MyBeanPostProcessor">
    </bean>

    <bean id="instantiationAwareBeanPostProcessor" class="com.example.mybatisplustest.beans.MyInstantiationAwareBeanPostProcessor">
    </bean>

    <bean id="beanFactoryPostProcessor" class="com.example.mybatisplustest.beans.MyBeanFactoryPostProcessor">
    </bean>

    <bean id="person" class="com.example.mybatisplustest.beans.Person" init-method="myInit"
          destroy-method="myDestory" scope="singleton" p:address="广州"  p:name="张三" p:phone="15900000000" >
    </bean>
</beans>